{
  "name": "@chl1860/dynamic-form-vue3",
  "version": "2.0.0",
  "type": "module",
  "description": "Vue3 + TypeScript + Ant Design Vue 动态表单组件",
  "main": "./dist/dynamic-form.umd.js",
  "module": "./dist/dynamic-form.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/dynamic-form.es.js",
      "require": "./dist/dynamic-form.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "vue",
    "vue3",
    "typescript",
    "dynamic-form",
    "form-builder",
    "ant-design-vue",
    "dynamic-fields",
    "form-linkage",
    "form-validation"
  ],
  "author": "Dynamic Form Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chl1860/dynamic-form.git"
  },
  "bugs": {
    "url": "https://github.com/chl1860/dynamic-form/issues"
  },
  "homepage": "https://github.com/chl1860/dynamic-form#readme",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --project tsconfig.build.json && vite build",
    "build:types": "vue-tsc --project tsconfig.build.json --declaration --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "npm run build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit",
    "publish:patch": "chmod +x scripts/publish.sh && ./scripts/publish.sh patch",
    "publish:minor": "chmod +x scripts/publish.sh && ./scripts/publish.sh minor",
    "publish:major": "chmod +x scripts/publish.sh && ./scripts/publish.sh major",
    "publish:quick": "chmod +x scripts/quick-publish.sh && ./scripts/quick-publish.sh patch",
    "publish:check": "npm run lint && npm run type-check && npm run test && npm run build"
  },
  "peerDependencies": {
    "ant-design-vue": "^4.0.0",
    "vue": "^3.4.0"
  },
  "optionalDependencies": {
    "@ant-design/colors": "^7.0.0",
    "@ant-design/icons-vue": "^7.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.19.0",
    "jsdom": "^24.0.0",
    "terser": "^5.20.0",
    "typescript": "~5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^1.0.0",
    "vue-tsc": "^1.8.0"
  }
}
